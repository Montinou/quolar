{
  "name": "@quolar/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Core workflow orchestrator and provider interfaces for Quolar",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
